<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <import resource="messages.xml"/>
  <import resource="themes.xml"/>

  <bean id="bean1" class="com.luxoft.springioc.example05.Bean1" p:message-ref="message"/>
  <bean id="bean2" class="com.luxoft.springioc.example05.Bean2"/>
  <bean id="theme" class="com.luxoft.springioc.example05.Theme" c:name="1"/>

</beans>
